{
  "name": "strapi-middleware-cache",
  "version": "2.1.7",
  "description": "Speed-up HTTP requests with LRU cache",
  "strapi": {
    "displayName": "REST Cache",
    "name": "strapi-middleware-cache",
    "description": "Speed-up HTTP requests with LRU cache",
    "kind": "plugin"
  },
  "main": "./",
  "types": "./types",
  "scripts": {
    "eslint:server": "eslint .",
    "eslint:server:fix": "eslint . --fix",
    "eslint:admin": "eslint ./src/admin/src",
    "eslint:admin:fix": "eslint ./src/admin/src --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "lint": "run-s eslint:server:fix eslint:admin:fix prettier:fix",
    "test": "run-s test:*",
    "test:tsc": "tsc",
    "test:lint": "run-s eslint:server eslint:admin prettier",
    "test:mocha": "mocha ./tests/*.test.js --exit"
  },
  "keywords": [
    "cache",
    "strapi",
    "requests",
    "redis",
    "memcache"
  ],
  "directories": {
    "lib": "./lib",
    "types": "./types"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/patrixr/strapi-middleware-cache.git"
  },
  "author": "Patrick R",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.2",
    "ioredis": "^4.28.2",
    "koa-router": "^10.1.1",
    "lodash": "^4.17.15",
    "lru-cache": "^6.0.0",
    "redis-lru": "^0.6.0"
  },
  "devDependencies": {
    "@strapi/strapi": "^4.0.3",
    "@babel/eslint-parser": "^7.5.4",
    "@babel/preset-react": "^7.16.7",
    "chai": "^4.2.0",
    "codecov": "^3.8.3",
    "eslint": "^8.6.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^7.0.0",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-loader": "4.0.2",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-flowtype": "8.0.3",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^37.5.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-redux-saga": "^1.3.2",
    "jest": "^27.4.7",
    "jest-cli": "^27.4.7",
    "jest-styled-components": "^7.0.2",
    "koa": "^2.11.0",
    "lint-staged": "^12.1.7",
    "lodash": "^4.17.11",
    "mocha": "^9.1.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-intl": "^5.20.12",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "sinon": "^12.0.1",
    "styled-components": "^5.2.3",
    "supertest": "^6.1.6",
    "supertest-koa-agent": "^0.3.2",
    "yorkie": "^2.0.0"
  },
  "peerDependencies": {
    "@strapi/strapi": "^4.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
